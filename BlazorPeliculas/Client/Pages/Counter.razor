@page "/counter"


<h1>Counter</h1>

<p>Current count: @currentCount</p>
<p>singleton count: @singleton.Valor</p>
<p>transient count: @transient.Valor</p>

<button class="btn btn-primary" @onclick="IncrementCountAsync">Click me</button>


<button class="btn btn-primary" @onclick="IncrementCountAsyncJavascript">Click me javascript</button>


